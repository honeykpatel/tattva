<h2>All Persons</h2>

<form action="/persons" method="get">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% persons.forEach(person => { %>
    <div>
        <%= person.name %> 
    </div>
<% }) %> 